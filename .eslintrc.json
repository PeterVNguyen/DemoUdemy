{
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "linebreak-style": [2, "unix"],
    "curly": [2, "all"],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "eqeqeq": [2, "smart"],
    "one-var-declaration-per-line": [2, "always"],
    "new-cap": 2,
    "no-case-declarations": 0
  }
}
